{
    "name": "krunkscript-hightlighter",
    "displayName": "KrunkScript Highlighter",
    "description": "A syntax highlighter for KrunkScript",
    "icon": "./img/icon.png",
    "version": "1.0.0",
    "publisher": "KPal81",
    "private": true,
    "license": "Proprietary",
    "engines": {
        "vscode": "^1.56.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "krnk",
            "aliases": ["KrunkScript", "krnk"],
            "extensions": [".krnk"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "krnk",
            "scopeName": "source.krnk",
            "path": "./syntaxes/krnk.tmLanguage.json"
        }]
    }
}